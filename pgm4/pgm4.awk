BEGIN {
c1 = 0
c2 = 0
p1 = 0
p2 = 0
t1 = 0
t2 = 0
}

{
if($1=="r" && $3=="_1_" && $4=="AGT"){
c1++;
p1 = p1+$8
t1=$2
}

if($1=="r" && $3=="_2_" && $4=="AGT"){
c2++;
p2 = p2+$8
t2=$2
}

}

END {

printf("N0 to n1: %f Mbps\n", ((c1*p1*8)/(t1*1000000)));
printf("N1 to n2: %f Mbps\n", ((c2*p2*8)/(t2*1000000)));
}
